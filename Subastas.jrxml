	<queryString>
		<![CDATA[SELECT
  categoria.nombre_categoria,
  sum (articulo.precio_articulo),
  articulo.estado_articulo
FROM
  public.articulo,
  public.categoria
WHERE
  categoria.id_categoria = articulo.id_categoria
  group by (categoria.nombre_categoria,articulo.estado_articulo)]]>
